<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rudzko.firstapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.rudzko.firstapp.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.rudzko.firstapp.hw_1.hw1_Activity"/>
        <activity android:name="com.rudzko.firstapp.hw_2.hw2_Activity"/>
        <activity android:name="com.rudzko.firstapp.hw_3.hw3_Activity"/>
        <activity android:name="com.rudzko.firstapp.cw_2.cw2_Activity"/>
        <activity android:name="com.rudzko.firstapp.cw_3.cw3_Activity"/>

    </application>

</manifest>